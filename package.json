{
  "name": "magwords",
  "version": "0.0.0-development",
  "description": "It's written in Rust!",
  "main": "src/main.rs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest --coverage.enabled=true",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "release": "semantic-release",
    "deps:ci": "depcruise --output-type err front-end/src",
    "deps:graph": "depcruise --output-type dot front-end/src | dot -T svg > dependency-graph.svg",
    "deps:report": "depcruise --output-type err-html --output-to dependency-report.html front-end/src"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kristof-mattei/magwords"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kristof-mattei/magwords/issues"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jss": "^10.10.0",
    "@tanstack/react-query": "^5.17.12",
    "xss": "^1.0.14"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix -- ",
      "depcruise --include-only ^src/ --validate -- "
    ],
    "*": [
      "prettier --ignore-unknown --write --"
    ]
  },
    "homepage": "https://github.com/kristof-mattei/magwords#readme",
  "devDependencies": {
    "@actions/tool-cache": "2.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.1.0",
    "@semantic-release/github": "9.2.6",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@types/node": "20.11.3",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.2.0",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "dependency-cruiser": "16.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hook-form": "0.3.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "jest-environment-jsdom": "29.7.0",
    "nock": "13.5.0",
    "prettier": "3.2.2",
    "semantic-release": "23.0.0",
    "semver": "7.5.4",
    "serialize-error": "11.0.3",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-checker": "0.6.2",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.2.3",
    "vitest": "1.2.0"

  },
  "publishConfig": {
    "access": "restricted"
  }
}
